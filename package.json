{
  "name": "eztexting-node",
  "version": "0.0.12",
  "description": "Send async single, bulk and bulk individual SMS and MMS node-libcurl using the EZTexting REST API.",
  "main": "./build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "rmi": "nodemon src/rmi/masstext.ts",
    "test": "nodemon src/tests.ts",
    "test-messages": "nodemon src/test.messages.ts",
    "test-qrcode": "nodemon src/test.qrcode.ts",
    "test-file": "nodemon src/test.mediafiles.ts",
    "build": "rimraf build && tsc --project tsconfig.json && npm run copy",
    "copy": "copyfiles --up 2 src/conf/cacert.pem build/conf"
  },
  "keywords": [
    "node",
    "eztext",
    "eztexting",
    "sms",
    "mms",
    "bulk-texting",
    "text",
    "messages"
  ],
  "author": "River University",
  "license": "MIT",
  "homepage": "https://github.com/riveruniversity/eztexting-node#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riveruniversity/eztexting-node.git"
  },
  "bugs": {
    "url": "https://github.com/riveruniversity/eztexting-node/issues"
  },
  "bin": {
    "eztexting-node": "./build/index.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "canvas": "^2.9.1",
    "dotenv": "^16.0.1",
    "fs-extra": "^10.1.0",
    "jsdom": "^19.0.0",
    "node-libcurl": "^3.0.0",
    "qr-code-styling-node": "^1.5.0",
    "rxjs": "^7.5.7"
  },
  "devDependencies": {
    "@types/node": "^17.0.36",
    "@types/qrcode": "^1.4.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.16.0",
    "nodemon": "^2.0.16",
    "rimraf": "^5.0.1",
    "ts-node": "^10.8.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  }
}
